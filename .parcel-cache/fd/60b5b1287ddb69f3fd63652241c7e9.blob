#confetti{
  opacity: 0;
  position: fixed;
  left: 0;
  top: 0;
  transition: all 5s;
}

#confetti.opa{
  opacity: 1;
}

.header{
  margin-top: 20px;
  width: 100%;
  opacity: 0;
  display: flex;
  justify-content: space-between;
}

.header.opac{
  opacity: 1;
  transition: all 1s;
  transition-delay: 0.5s;
}


@keyframes confetti-ani {
  0%   { opacity: 0; }
  100% { opacity: 1; }
}

  
    

    strong {
      font-weight: 500; 
    }
    




a {
    text-decoration: none;
    color: black;
}

.page {
    margin-bottom: 0px;
    margin-top: 50px;
    width: 100%;
    position: relative;
    transition: 1s all;
    display: flex;
    flex-direction: column;
    transform: translateY(200px);
    opacity: 0%;
    -webkit-transition: all 0.4s ease;
    transition: all 0.4s ease;
}

.page.move{
    transform: translateY(0px);
    opacity: 100%;

}


.group {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.element {
    position: relative;
    transition: 0.5s all;
}

.hidden {
    display: none;
}

.message-block {
    line-height: 26px;
    position: relative;
    transition: 0.5s all;
    -webkit-transition: 0.5s all;
    -moz-transition: 0.5s all;
    -ms-transition: 0.5s all;
    -o-transition: 0.5s all;
}

.message-block-last {
    margin-bottom: 15px; 
}

.right .message,
.left .message {
    position: relative;
    transition: 1s opacity, all 0.4s;
    -webkit-transition: 1s opacity, all 0.4s;
    -moz-transition: 1s opacity, all 0.4s;
    -ms-transition: 1s opacity, all 0.4s;
    -o-transition: 1s opacity, all 0.4s;
    opacity: 0;
}

@keyframes bubbleleft {
    0% {
        opacity: 0;
        transform: rotate(-45deg) scaleX(-1);
        left: -3.5px;
    }

    100% {
        opacity: 1;
        transform: rotate(0) scaleX(-1);
    }
}

@keyframes bubbleRight {
    0% {
        opacity: 0;
        transform: rotate(45deg) scaleX(1);
        right: -4.5px;
    }

    100% {
        opacity: 1;
        transform: rotate(0) scaleX(1);
    }
}

.left>svg {
    position: absolute;
    left: -15.5px;
    bottom: -1px;
    transform: rotate(-45deg) scaleX(-1);
    animation: bubbleleft 0.4s ease 0.2s;
    animation-fill-mode: forwards;
    animation-delay: 0.6s;
    fill: var(--left-svg-fill);
    opacity: 0;
}

.right>svg {
    position: absolute;
    right: -16.5px;
    bottom: -2px;
    transform: rotate(45deg) scaleX(1);
    animation: bubbleRight 0.4s ease 0.2s;
    animation-fill-mode: forwards;
    animation-delay: 0.6s;
    fill: #0253e2;
    opacity: 0;
}

.message-block .show {
    opacity: 1;
}

.right[data-emergence=hidden] .message {
    left: 500px;
}

.right[data-emergence=visible] .message {
    left: 0;
    opacity: 1;
}

.left[data-emergence=hidden] .message {
    left: -500px;
}

.left[data-emergence=visible] .message {
    left: 0px;
    opacity: 1;
}

.message-block {
    display: block;
    width: 100%;
    max-width: 675px; 
   /* max-width: 860px; */
    margin-bottom: 10px;
}

.message {
    border-radius: 30px;
    display: inline-block;
    padding: 15px 25px;
    line-height: 34px;
    font-size: 24px;
}

.right .message {
    background-color: #0253e2;
    color: white;
    float: right;
}

.left .message {
    color: var(--left-message-color);
    background-color: var(--left-message-bg-color);
}


.button-group {
    display: flex;
    justify-content: center;
    margin-top: 75px;
    margin-bottom: 75px;
}


.btn {
    margin: 0 10px;
    background-color: transparent;
    padding: 10px 20px;
    border: var(--btn-border);
    transition: 0.3s all;
    font-size: 20px;
    color: var(--btn-text-color);
    border-radius: 50px;
}

.btn:hover {
    background-color: var(--btn-bg-color-hover);
    cursor: pointer;
}

.btn:focus {
    border-color: var(--border-color);
    outline: none;
}






.profile{
    transform: scale(0.5);
    opacity: 0%;
    margin-top: 25%;
    position: relative;
    display: block;
    margin-left: auto; margin-right: auto;
    width: 200px;
    height: 200px;
 		-webkit-transition: all 0.4s ease;
 		transition: all 0.4s ease;
   }

.profile.animate{
    transform: scale(1);
    opacity: 100%;
}

.profile.animate.smaller{
    margin-top: 23px;
    position: relative;
    display: block;
    left: 2%;
    width: 90px;
    height: 90px;
}
   
