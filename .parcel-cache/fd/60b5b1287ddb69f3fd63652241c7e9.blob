.box {
  position: relative;
  display: block;
  margin-right: 20px;
  margin-left: 20px;
  opacity: 0;
  transition: all 0.5s;
  transform: translateY(50px);
}

.box.load {
  opacity: 1;
  transform: translateY(0);
}

.box.a{
  transition-delay: 0.5s;;
}

.box.b{
  transition-delay: 1.05s;;
}

.box.c{
  transition-delay: 1.60s;;
}



.box img{
  -webkit-filter: brightness(100%);
  margin-top: 20px;
   cursor: pointer;
   max-width: 230px; /* 250 */
}


.box img:hover {
-webkit-filter: brightness(70%);
-webkit-transition: all 1s ease;
-moz-transition: all 1s ease;
-o-transition: all 1s ease;
-ms-transition: all 1s ease;
transition: all 0.6s ease;
}


   .gallery-h{
     font-size: 21px;
     font-weight: 500;
     color: var(--text-link-colors);
     display: block;
     width: 150px;
     margin-bottom: 0px;
     
   }

   .gallery-h:hover{
      color: #B130FF;
      transition: all 0.35s;
      text-decoration: underline;
   }

   .gallery-p{
    font-weight: 400;
    color: var(--text-link-colors);
    opacity: 0.5;
    margin-top: 3px;
    display: block;
    font-size: 16px;;
  }
   
.phone-link{
  text-decoration: underline;
  color: #007aff;
}

#pdf{
  text-decoration: underline;
  color: var(--border-color);
  cursor: pointer;
  line-height: 1;
  margin-left: auto;
  transition: all 1s;
}

#pdf::after{
  content: "ðŸ‘‰";
